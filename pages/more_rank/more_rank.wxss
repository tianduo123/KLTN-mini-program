/* pages/more_rank/more_rank.wxss */
page{
  background: #f5f5f5
}
.top{
  width: 100%;
  height: 370rpx;
  background: linear-gradient(to bottom,#fd394e,#ff880c);
  text-align: center;
  box-sizing: border-box;
  padding-top: 60rpx  
}
.title{
  font-size: 26rpx;
  color: #fff
}
.rank{
  margin-top: 30rpx;
  font-size: 50rpx;
  font-weight: bold;
  color: #fff
}
.userInfo{
  width: 90%;
  height: 150rpx;
  border-radius: 20rpx;
  background: #fff;
  margin: -100rpx auto 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between
}
.rankList{
  width: 90%;
  height: 500rpx;
  border-radius: 20rpx;
  background: #fff;
  margin: 0 auto
}
.left{
  display: flex;
  align-items: center;
  margin-left: 30rpx
}
.right{
  display: flex;
  align-items: center;
  margin-right: 30rpx
}